<?xml version="1.0" encoding="utf-8"?>

<DTO-mapping>
    <class name="cn.com.hd.dto.navy.THighWay" table="T_HIGHWAY" schema="NAVYSYS">
    	<comment>高速公路信息表</comment>
        <id name="comid" type="VARCHAR2">
            <column name="COM_ID" length="36">
            	<comment>运输服务企业ID</comment> 
            </column>
        </id>
        <property name="hiwid" type="VARCHAR2">
            <column name="hiwid" length="32">
            	<comment>高速公路编号</comment>
            </column>
        </property>
        <property name="hiwname" type="VARCHAR2">
            <column name="HIW_NAME" length="64">
            	<comment>高速公路名称</comment>
            </column>
        </property>	
        <property name="hiwin" type="VARCHAR2">
            <column name="HIW_IN" length="64">
            	<comment>高速公路入口名称</comment>
            </column>
        </property>
        <property name="hiwinid" type="VARCHAR2">
            <column name="HIW_IN_ID" length="32">
            	<comment>高速公路入口编号</comment>
            </column>
        </property>
        <property name="hiwdis" type="DOUBLENUMBER">
            <column name="HIW_DIS" precision="10" scale="2">
            	<comment>距离</comment>
            </column>
        </property>		
    </class>	
</DTO-mapping>
