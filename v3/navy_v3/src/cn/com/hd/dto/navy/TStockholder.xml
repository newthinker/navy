<?xml version="1.0" encoding="utf-8"?>

<DTO-mapping>

    <class name="cn.com.hd.dto.navy.TStockholder" table="T_STOCKHOLDER" schema="NAVYSYS">
        <comment>供应商股东信息表</comment>
        <id name="stockholderid" type="VARCHAR2">
            <column name="STOCKHOLDER_ID" length="36" />
        </id>
        <property name="supid" type="VARCHAR2">
            <column name="SUP_ID" length="36">
                <comment>供应商ID</comment>
            </column>
        </property>
        <property name="stockholdername" type="VARCHAR2">
            <column name="STOCKHOLDER_NAME" length="100">
                <comment>股东姓名</comment>
            </column>
        </property>
        <property name="capital" type="DOUBLENUMBER">
            <column name="CAPITAL" precision="8">
                <comment>出资金额</comment>
            </column>
        </property>
        <property name="ratio" type="VARCHAR2">
            <column name="RATIO" length="50">
                <comment>出资比例</comment>
            </column>
        </property>
        <property name="stockdate" type="DATE">
            <column name="STOCK_DATE" length="7">
                <comment>出资时间</comment>
            </column>
        </property>
    </class>
    
</DTO-mapping>
