            </column>
        </property>
        <property name="singlemaxdate" type="VARCHAR2">
            <column name="SINGLE_MAX_DATE" length="200">
                <comment>完成单批最大产量所用时间</comment>
            </column>
        </property>
        <property name="remark" type="VARCHAR2">
            <column name="REMARK" length="1000">
                <comment>备注</comment>
            </column>
        </property>
    </class>
    
</DTO-mapping>
