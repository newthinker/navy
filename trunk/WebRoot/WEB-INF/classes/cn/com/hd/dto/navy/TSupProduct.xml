<?xml version="1.0" encoding="utf-8"?>

<DTO-mapping>

    <class name="cn.com.hd.dto.navy.TSupProduct" table="T_SUP_PRODUCT" schema="NAVYSYS">
        <comment>供应商产品表</comment>
        <id name="prodid" type="VARCHAR2">
            <column name="PROD_ID" length="36" />
        </id>
        <property name="supid" type="VARCHAR2">
            <column name="SUP_ID" length="36">
                <comment>供应商ID</comment>
            </column>
        </property>
        <property name="goodname" type="VARCHAR2">
            <column name="GOOD_NAME" length="200">
                <comment>物资品名</comment>
            </column>
        </property>
        <property name="prodname" type="VARCHAR2">
            <column name="PROD_NAME" length="200">
                <comment>产品名称</comment>
